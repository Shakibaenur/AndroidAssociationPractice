<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidAssociation"
        tools:targetApi="31" >
        <activity
            android:name=".practice.LemonadeActivity"
            android:exported="true"
            android:label="@string/title_activity_dice_roller"
            android:theme="@style/Theme.AndroidAssociation" >

        </activity>
        <activity
            android:name=".practice.ScrollableListActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AndroidAssociation" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".practice.DiceRollerActivity"
            android:exported="true"
            android:label="@string/title_activity_dice_roller"
            android:theme="@style/Theme.AndroidAssociation" >

        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AndroidAssociation" >

        </activity>
    </application>

</manifest>